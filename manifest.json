{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "3.0",
  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts":["background.js", "jquery-2.2.0.js"]
      },
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*"],
      "js": ["content.js", "jquery-2.2.0.js"]
    }
  ]
}
