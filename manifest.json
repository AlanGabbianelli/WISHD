{
  "manifest_version": 2,
  "name": "WISH'D",
  "version": "0.1",
  "browser_action": { "default_icon": "images/icon.png" },
  "permissions": ["storage", "tabs"],
  "background": { "scripts":["background.js"] },
  "content_scripts": [
    {
      "matches": [ "https://www.facebook.com/*", "https://twitter.com/*"],
      "js": ["content.js"]
    }
]}
